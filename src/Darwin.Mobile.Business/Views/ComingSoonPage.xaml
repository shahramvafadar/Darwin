<?xml version="1.0" encoding="utf-8"?>
<ContentPage
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:res="clr-namespace:Darwin.Mobile.Business.Resources"
    x:Class="Darwin.Mobile.Business.Views.ComingSoonPage"
    Title="{x:Static res:AppResources.ComingSoonTitle}">
    <VerticalStackLayout VerticalOptions="Center" HorizontalOptions="Center">
        <Label Text="{x:Static res:AppResources.ComingSoonTitle}"
               Style="{StaticResource TitleLabelStyle}" />
    </VerticalStackLayout>
</ContentPage>
